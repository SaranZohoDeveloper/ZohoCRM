// Co Owner Information //
CoOwner_firstname = ZDK.Page.getField('Co_Owner_First_Name');
CoOwner_lastname = ZDK.Page.getField('Co_Owner_Last_Name');
if (CoOwner_firstname.getValue() != null || CoOwner_lastname.getValue() != null)
{
    var Coownership_percentage = ZDK.Page.getField('Co_Ownership_Percentage');
    if (Coownership_percentage.getValue() == null)
    {
        Coownership_percentage.addTooltip("This field can't be empty");
    }

    var Coowner_street = ZDK.Page.getField('Co_Owner_Street');
    if (Coowner_street.getValue() == null)
    {
        Coowner_street.addTooltip("This field can't be empty");
    }

    var Coowner_city = ZDK.Page.getField('Co_Owner_City');
    if (Coowner_city.getValue() == null)
    {
        Coowner_city.addTooltip("This field can't be empty");
    }
    var Coowner_state = ZDK.Page.getField('Co_Owner_State');
    if (Coowner_state.getValue() == null)
    {
        Coowner_state.addTooltip("This field can't be empty");
    }

    var Coowner_zipcode = ZDK.Page.getField('Co_Owner_Zip_Code');
    if (Coowner_zipcode.getValue() == null)
    {
        Coowner_zipcode.addTooltip("This field can't be empty");
    }

    var Coowner_ssn = ZDK.Page.getField('Co_Owner_Social');
    if (Coowner_ssn.getValue() == null)
    {
        Coowner_ssn.addTooltip("This field can't be empty");
    }
    var Coowner_dob = ZDK.Page.getField('Co_Owner_Date_Of_Birth');
    if (Coowner_dob.getValue() == null)
    {
        Coowner_dob.addTooltip("This field can't be empty");
    }

     var Coowner_phone = ZDK.Page.getField('Co_Owner_Phone');
    if (Coowner_phone.getValue() == null)
    {
        Coowner_phone.addTooltip("This field can't be empty");
    }
     var Coowner_dob = ZDK.Page.getField('Co_Owner_Date_Of_Birth');
    if (Coowner_dob.getValue() == null)
    {
        Coowner_dob.addTooltip("This field can't be empty");
    }
     var Coowner_Email = ZDK.Page.getField('Co_Owner_Email');
    if (Coowner_Email.getValue() == null)
    {
        Coowner_Email.addTooltip("This field can't be empty");
    }
    if (Coownership_percentage.getValue() == null ||  Coowner_street.getValue() == null ||  Coowner_city.getValue() == null || Coowner_state.getValue() == null || Coowner_zipcode.getValue() == null || Coowner_ssn.getValue() == null || Coowner_dob.getValue() == null || Coowner_phone.getValue() == null ||  Coowner_dob.getValue() == null || Coowner_Email.getValue() == null )
    {
ZDK.Page.getButtons().find(a => a.getLabel() == "Convert").disable();
    }
    else
    {
ZDK.Page.getButtons().find(a => a.getLabel() == "Convert").enable();
    }

}

if (Company == null || Legal_entity.getValue() == null || Business_phone.getValue() == null || Business_email_address.getValue() == null || Get_Industry.getValue() == null || Physical_address.getValue() == null || Physical_city.getValue() == null || Physical_state.getValue() == null || Physical_zipcode.getValue() == null || Credit_card_processor.getValue() == null || Credit_card_monthly_volume.getValue() == null || Doin_business_As.getValue() == null || Federal_tax_id.getValue() == null || Business_website.getValue() == null || Business_start_date.getValue() == null || State_of_incorporation.getValue() == null
|| Mailing_address.getValue() == null || Mailing_zip_code.getValue() == null || Rent_own_business.getValue() == null || Rent_mortgage_amt.getValue() == null
|| Business_property_val.getValue() == null || Annual_gross_sale.getValue() == null || Ownership_percentage.getValue() == null || Owner_Street.getValue() == null || Owner_City.getValue() == null  || Owner_state.getValue() == null || Owner_zipcode.getValue() == null  || Owner_mobile.getValue() == null || Owner_email.getValue() == null || Owner_ssn.getValue() == null || Date_of_birth.getValue() == null || Credit_score_fico.getValue() == null || Rent_own_home.getValue() == null || Property_home_value.getValue() == null || Balance_left_home_mortgage.getValue() == null )
{

    //ZDK.Client.showAlert("Disable");
    ZDK.Page.getButtons().find(a => a.getLabel() == "Convert").disable();
   

}
else {

    //ZDK.Client.showAlert("enable");
    ZDK.Page.getButtons().find(a => a.getLabel() == "Convert").enable();
    
}

